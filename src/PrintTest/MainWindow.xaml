<Window
    x:Class="PrintTest.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Width="800"
    Height="450">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Click="OnPrintClick">Print</Button>
        <Canvas
            Name="Canvas"
            Grid.Row="1"
            Background="White">
            <TextBlock
                Canvas.Left="65"
                Canvas.Top="51"
                Text="Hello There" />
        </Canvas>
    </Grid>
</Window>